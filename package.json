{
  "name": "node-ts",
  "version": "1.0.0",
  "description": "a nodejs + typescript starter template repository",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch & node --watch --env-file=.env.local dist/index.js",
    "start": "npm run lint && npm run build && node --env-file=.env.local dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint --fix src --ext .ts"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "express"
  ],
  "author": "https://github.com/maxime-aube",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "imports": [],
  "exports": []
}
